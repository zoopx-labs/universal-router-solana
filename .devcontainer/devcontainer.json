{
  "name": "universal-router-solana",
  "context": "..",
  "dockerFile": "Dockerfile",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "rust-lang.rust-analyzer",
        "ms-vscode.cpptools",
        "ms-azuretools.vscode-docker",
        "ms-vscode-remote.remote-containers"
      ]
    }
  },
  "postCreateCommand": "python3 tools/generate_golden_vectors/generate.py > programs/zpx_router/tests/golden/hashes.json || true && cargo build -p zpx_router",
  "remoteUser": "root"
}
